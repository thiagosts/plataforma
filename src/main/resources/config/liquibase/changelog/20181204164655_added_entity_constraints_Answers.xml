<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Answers.
    -->
    <changeSet id="20181204164655-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="results_details_id"
                                 baseTableName="answers"
                                 constraintName="fk_answers_results_details_id"
                                 referencedColumnNames="id"
                                 referencedTableName="results_details"/>

        <addForeignKeyConstraint baseColumnNames="questions_id"
                                 baseTableName="answers"
                                 constraintName="fk_answers_questions_id"
                                 referencedColumnNames="id"
                                 referencedTableName="questions"/>

    </changeSet>
</databaseChangeLog>
